{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@icon/*": [
        "src/icons/*"
      ],
      "@layout/*": [
        "src/layouts/*"
      ],
      "@view/*": [
        "src/components/view/*"
      ],
      "@core/*": [
        "src/components/core/*"
      ],
      "@util/*": [
        "src/components/util/*"
      ],
      "@shared/*": [
        "src/components/shared/*"
      ],
      "@content/*": [
        "src/components/content/*"
      ],
      "@container/*": [
        "src/components/container/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}